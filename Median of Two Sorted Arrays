class Solution(object):
    def findMedianSortedArrays(self, nums1, nums2):
            nums3=nums1+nums2
            l3=len(nums3)
            nums3.sort()
            if(l3%2==0):
                median=(nums3[l3/2]+nums3[(l3/2)-1])/2.0
            else:
                median=(nums3[(l3//2)])
            return median
