class Solution {
public:
    int maxArea(vector<int>& height) {
        int i=0;
        int n=height.size();
        int j=n-1;
        int ma=0;
        while(i<j){
            int mi=min(height[i],height[j]);
            ma=max(ma,mi*(j-i));
            if(height[i]<height[j]){
                i=i+1;
            }else{
                j=j-1;
            }
        }return ma;
    }
};
