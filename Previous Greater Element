class Solution {
  public:
    vector<int> preGreaterEle(vector<int>& arr) {
        //  code here
        int n=arr.size();
        vector<int> arr2(n);
        stack<int> s;
        for (int i=0;i<n;i++){
            while (!s.empty() && s.top() <= arr[i]) {
            s.pop();
            }
            if(s.empty()){
                arr2[i]=-1;
            }else{
                arr2[i]=s.top();
            }
            s.push(arr[i]);
            
        }
    return arr2;
    }
};
