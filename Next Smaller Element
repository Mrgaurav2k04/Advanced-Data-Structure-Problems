class Solution {
  public:
    vector<int> nextSmallerEle(vector<int>& arr) {
        //  code here
        stack<int> s;
        int n=arr.size();
        vector<int> arr2(n);
        for(int i=n-1;i>=0;i--){
            while(!s.empty() && arr[s.top()]>=arr[i]){
                s.pop();
            }arr2[i]=s.empty()?-1:arr[s.top()];
            s.push(i);
        }return arr2;
    }
};
